{
    "name": "@yehezkieldio/artemis",
    "version": "1.0.2",
    "packageManager": "bun@1.2.13",
    "description": "CLI orchestrator for semantic versioning, changelog generation, and creating releases.",
    "type": "module",
    "license": "MIT",
    "homepage": "https://github.com/yehezkieldio/artemis#readme",
    "bugs": {
        "url": "https://github.com/yehezkieldio/artemis/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yehezkieldio/artemis.git"
    },
    "author": "Yehezkiel Dio Sinolungan <yehezkieldio@proton.me>",
    "funding": "https://www.nihbuatjajan.com/yehezkieldio",
    "files": [
        "dist"
    ],
    "main": "./dist/cli.js",
    "module": "./dist/cli.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./*"
            ]
        }
    },
    "bin": {
        "artemis": "./dist/cli.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "start:debug": "bun run src/cli.ts",
        "lint": "biome check .",
        "lint:fix": "biome check . --fix --unsafe",
        "format": "biome format . --write",
        "typecheck": "tsc --noEmit",
        "build": "tsdown"
    },
    "dependencies": {
        "@gitbeaker/rest": "^42.5.0",
        "@octokit/auth-token": "^6.0.0",
        "@octokit/core": "^7.0.0",
        "c12": "^3.0.3",
        "commander": "^14.0.0",
        "consola": "^3.4.2",
        "conventional-recommended-bump": "^11.0.0",
        "git-cliff": "^2.8.0",
        "neverthrow": "^8.2.0",
        "semver": "^7.7.1",
        "smol-toml": "^1.3.3"
    },
    "devDependencies": {
        "@biomejs/biome": "2.1.2",
        "@types/bun": "^1.2.12",
        "@types/node": "^24.0.0",
        "@types/semver": "^7.7.0",
        "tsdown": "^0.12.0",
        "typescript": "^5.8.3"
    }
}
